<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>HelloServletAjax</servlet-name>
        <servlet-class>com.wjx.servlet.HelloServletAjax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HelloServletAjaxMethod</servlet-name>
        <servlet-class>com.wjx.servlet.HelloServletAjaxMethod</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HelloServletAjaxResponse</servlet-name>
        <servlet-class>com.wjx.servlet.HelloServletAjaxResponse</servlet-class>
    </servlet>
    
    
    <servlet-mapping>
        <servlet-name>HelloServletAjax</servlet-name>
        <url-pattern>/HelloServletAjax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HelloServletAjaxMethod</servlet-name>
        <url-pattern>/HelloServletAjaxMethod</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HelloServletAjaxResponse</servlet-name>
        <url-pattern>/HelloServletAjaxResponse</url-pattern>
    </servlet-mapping>
</web-app>